  "fase_comissao": [
    {"fase": "analise_do_relator", "code": [91], "regex":"(designa.*relator(a|) da matéria)|((à|ao|a).* senador(a|).*para ((emitir|emissão de) relatório|relatar a presente matéria.*))"},
